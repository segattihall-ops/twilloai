# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number
BRAZILIAN_BLESSED_TEAM_NUMBER=your_team_phone_number

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# ElevenLabs Configuration (for voice synthesis)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_VOICE_ID=your_voice_id_for_sarah

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Vercel Configuration (automatically set by Vercel)
VERCEL_URL=your_vercel_deployment_url

# OpenAI Realtime API WebSocket Server (optional - for lower latency)
# Deploy the server from /server directory and add the WebSocket URL here
# Example: wss://your-app.railway.app
REALTIME_WEBSOCKET_URL=

# Zapier Webhook Configuration (for call data collection)
ZAPIER_CALL_DATA_WEBHOOK=https://hooks.zapier.com/hooks/catch/your-webhook-id/
ZAPIER_ESTIMATE_WEBHOOK=https://hooks.zapier.com/hooks/catch/your-estimate-webhook-id/